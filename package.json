{
  "name": "kbb-instal",
  "private": true,
  "version": "0.0.5",
  "module": "./dist/kbb-instal.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/kbb-instal.js"
    },
    "./dist/": {
      "import": "./dist/"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc & vite build",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint:scripts": "eslint . --ext .{ts,tsx,js,jsx}",
    "lint:styles": "eslint . --ext .{css}",
    "lint": "npx eslint .",
    "format:check": "prettier . --check",
    "format:fix": "prettier . --write",
    "format": "npm run format:check && npm run format:fix",
    "prepare": "husky && echo 'npx lint-staged' > .husky/pre-commit && git add .husky/pre-commit",
    "uninstall-husky": "npm uninstall husky --no-save && git config --unset core.hooksPath && npx rimraf .husky"
  },
  "devDependencies": {
    "@eslint/css": "^0.8.1",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.30",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitest/coverage-v8": "^3.0.7",
    "copyfiles": "^2.4.1",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-stylelint": "^0.1.1",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.3",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.5.2",
    "pretty-quick": "^4.1.1",
    "rimraf": "^6.0.1",
    "stylelint": "^16.14.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended": "^15.0.0",
    "stylelint-config-sass-guidelines": "^12.1.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0",
    "vite": "^6.2.0",
    "vitest": "^3.0.7"
  },
  "dependencies": {
    "@types/grecaptcha": "^3.0.9",
    "imask": "^7.6.1"
  }
}
